{
    "manifest_version": 3,
    "name": "Following is Best for Twitter",
    "version": "1.0.0",
    "description": "twitter.comにアクセスしたとき、自動的にフォロー中タブに切り替えます",
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/",
                "https://www.twitter.com/",
                "https://twitter.com/home",
                "https://www.twitter.com/home"
            ],
            "js": [
                "src/index.js"
            ]
        }
    ]
}